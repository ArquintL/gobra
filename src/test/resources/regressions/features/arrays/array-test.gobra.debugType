package main


type Rectangle (:: NamedType) struct {
  Width (:: Field), Height (:: Field) int
}

ghost pure
requires acc(r (:: ReceiverParameter).Width (:: Field))
ensures acc(r (:: ReceiverParameter).Width (:: Field))
func (r (:: ReceiverParameter) *Rectangle (:: NamedType)) GetWidth (:: MethodImpl)() (res (:: OutParameter) int) {
  return r (:: ReceiverParameter).Width (:: Field)
  
}

func main (:: Function)() () {
  !r (:: SingleLocalVariable) := Rectangle (:: NamedType) {Width (:: Field): 2,Height (:: Field): 5 }
  w (:: SingleLocalVariable) := r (:: SingleLocalVariable).GetWidth (:: MethodImpl)()
  assert *Rectangle (:: NamedType).GetWidth (:: MethodImpl)(&r (:: SingleLocalVariable)) == w (:: SingleLocalVariable) && w (:: SingleLocalVariable) == 2
  
}

