package pkg

func nilTest() {
  var x interface{}
  assert x == nil
}

func intTest() {
  var a, b int32 = 42, 42
  var x interface{} = a
  c := x.(int32)
  assert c == 42
  assert x != 42
  assert x == b
  assert b == x
}

type pair struct{
  left, right int
}

func structTest() {
  a := pair{1, 2}
  var x interface{} = a
  b := x.(pair)
  assert b == pair{1, 2}

  b.left = 3
  c := x.(pair)
  assert c == pair{1, 2}
  assert x == pair{1,2}
}

